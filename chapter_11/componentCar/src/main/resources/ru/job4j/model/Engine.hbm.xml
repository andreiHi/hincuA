<hibernate-mapping>
    <class name="ru.job4j.model.car.Engine" table="engine">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="fuel" column="fuel"/>
        <property name="volume" column="volume"/>
        <one-to-one name="car" class="ru.job4j.model.car.Car"/>
    </class>
</hibernate-mapping>