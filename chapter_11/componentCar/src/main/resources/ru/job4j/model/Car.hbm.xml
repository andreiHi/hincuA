<hibernate-mapping>
<class name="ru.job4j.model.Car" table="car">
    <id name="id" column="id">
        <generator class="identity"/>
    </id>
    <property name="name" column="name"/>
    <many-to-one name="engine" class="ru.job4j.model.Engine" column="id_engine" unique="true"/>
    <many-to-one name="transmission" class="ru.job4j.model.Transmission" column="id_transmission" unique="true"/>
    <many-to-one name="gearbox" class="ru.job4j.model.Gearbox" column="id_gearbox" unique="true"/>
</class>
</hibernate-mapping>